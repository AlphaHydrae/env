{
  "[dockercompose]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[elixir]": {
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.formatOnSave": true,
    "editor.formatOnType": false,
    "editor.trimAutoWhitespace": true,
    "editor.wordBasedSuggestions": "matchingDocuments",
    "files.insertFinalNewline": true
  },
  "[elm]": {
    "editor.codeLens": false,
    "editor.formatOnSave": true
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[jade]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[java]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma",
    "editor.formatOnSave": true
  },
  "[rust]": {
    "editor.formatOnSave": true
  },
  "[sql]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "better-comments.tags": [
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      "tag": "fixme",
      "color": "#c00000",
      "strikethrough": false,
      "backgroundColor": "transparent"
    }
  ],
  "debug.console.fontSize": 15,
  "debug.toolBarLocation": "docked",
  "editor.accessibilitySupport": "off",
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "editor.cursorStyle": "line",
  "editor.detectIndentation": false,
  "editor.foldingImportsByDefault": false,
  "editor.fontSize": 18,
  "editor.fontFamily": "BitStream Vera Sans Mono, Menlo, Monaco, 'Courier New', monospace",
  "editor.inlayHints.enabled": "onUnlessPressed",
  "editor.insertSpaces": true,
  "editor.lineNumbers": "on",
  "editor.quickSuggestions": {
    "other": "on",
    "comments": "off",
    "strings": "off"
  },
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [80],
  "editor.scrollBeyondLastLine": false,
  "editor.suggest.showStatusBar": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "[dark-plus-syntax]": {
      "comments": "#6A9955",
      "textMateRules": [
        {
          "scope": [
            "comment",
            "comment.line.double-slash.js",
            "comment.block.html",
            "comment.block.handlebars"
          ],
          "settings": {
            "foreground": "#6A9955"
          }
        },
        {
          "name": "Curly Braces, Semicolons, etc.",
          "scope": [
            "meta.tag.sgml.html",
            "meta.tag.sgml.doctype.html",
            "meta.tag.sgml.html punctuation.definition.tag.html",
            "meta.structure.dictionary.json",
            "punctuation.definition.array.begin.json",
            "punctuation.definition.array.end.json",
            "punctuation.separator.array.json",
            "punctuation.terminator",
            "punctuation.definition.string.begin.markdown",
            "punctuation.definition.string.end.markdown",
            "punctuation.definition.metadata.markdown",
            "invalid.illegal.bad-comments-or-CDATA.html",
            "punctuation.definition.italic.markdown",
            "punctuation.definition.bold.markdown"
          ],
          "settings": {
            "foreground": "#d4d4d4"
          }
        },
        {
          "scope": [
            "comment.line.number-sign.yaml",
            "punctuation.separator.key-value.mapping.yaml"
          ],
          "settings": {
            "foreground": "#6A9955"
          }
        },
        {
          "scope": [
            "markup.underline.link.markdown",
            "markup.underline.link.image.markdown"
          ],
          "settings": {
            "foreground": "#999900"
          }
        },
        {
          "scope": [
            "meta.link.reference.markdown constant.other.reference.link.markdown",
            "meta.image.reference.markdown constant.other.reference.link.markdown"
          ],
          "settings": {
            "fontStyle": "italic",
            "foreground": "#999900"
          }
        },
        {
          "scope": [
            "markup.fenced_code.block.markdown punctuation.definition.markdown",
            "punctuation.definition.italic.markdown",
            "punctuation.definition.bold.markdown",
            "punctuation.definition.raw.markdown",
            "beginning.punctuation.definition.quote.markdown",
            "meta.link.reference.def.markdown punctuation.definition.constant.markdown",
            "meta.link.reference.def.markdown punctuation.separator.key-value.markdown",
            "meta.link.reference.markdown punctuation.definition.constant.begin.markdown",
            "meta.link.reference.markdown punctuation.definition.constant.end.markdown",
            "meta.image.reference.markdown punctuation.definition.constant.markdown",
            "block-dollars",
            "inline-dollars"
          ],
          "settings": {
            "foreground": "#ffffff"
          }
        }
      ]
    }
  },
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "explorer.confirmDelete": true,
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": false,
  "files.associations": {
    "Vagrantfile": "ruby",
    ".env.sample": "dotenv"
  },
  "git.openRepositoryInParentFolders": "never",
  "github.copilot.editor.enableAutoCompletions": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "gitlens.currentLine.enabled": false,
  "java.configuration.updateBuildConfiguration": "automatic",
  "java.completion.favoriteStaticMembers": [
    "ch.sqrs.core.errors.BusinessException.*",
    "ch.sqrs.core.test.matchers.identifiable.ContainsIdentifiableMatcher.*",
    "ch.sqrs.gestimmo.core.utils.OptionalUtils.*",
    "ch.sqrs.gestimmo.core.utils.Some.*",
    "com.jayway.jsonpath.matchers.JsonPathMatchers.*",
    "com.spotify.hamcrest.optional.OptionalMatchers.*",
    "java.util.Collections.*",
    "java.util.function.Predicate.*",
    "java.util.Objects.*",
    "java.util.stream.Collectors.*",
    "javax.persistence.*",
    "org.apache.commons.lang3.StringUtils.*",
    "org.exparity.hamcrest.date.LocalDateMatchers.*",
    "org.exparity.hamcrest.date.LocalDateTimeMatchers.*",
    "org.exparity.hamcrest.date.LocalTimeMatchers.*",
    "org.junit.Assert.*",
    "org.junit.jupiter.api.Assertions.*",
    "org.junit.jupiter.api.Assumptions.*",
    "org.hamcrest.CoreMatchers.*",
    "org.hamcrest.MatcherAssert.*",
    "org.hamcrest.Matchers.*",
    "org.mockito.Mockito.*",
    "org.mockito.AdditionalAnswers.*",
    "org.mockito.AdditionalMatchers.*",
    "org.mockito.ArgumentMatchers.*",
    "org.mockito.hamcrest.MockitoHamcrest.*",
    "org.springframework.data.jpa.domain.Specification.*",
    "org.springframework.test.web.client.match.MockRestRequestMatchers.*",
    "org.springframework.test.web.client.response.MockRestResponseCreators.*",
    "org.springframework.test.web.servlet.ResultMatcher.*",
    "org.springframework.test.web.servlet.request.MockMvcRequestBuilders.*",
    "org.springframework.test.web.servlet.result.MockMvcResultMatchers.*"
  ],
  "java.import.generatesMetadataFilesAtProjectRoot": false,
  "java.inlayHints.parameterNames.enabled": "all",
  "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  "javascript.inlayHints.parameterNames.enabled": "all",
  "javascript.inlayHints.parameterTypes.enabled": true,
  "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
  "javascript.inlayHints.variableTypes.enabled": true,
  "javascript.preferences.quoteStyle": "single",
  "jest.runMode": "on-demand",
  "Lua.telemetry.enable": false,
  "prettier.requireConfig": true,
  "prisma.showPrismaDataPlatformNotification": false,
  "redhat.telemetry.enabled": false,
  "rust-client.rustupPath": "~/.asdf/shims/rustup",
  "sonarlint.rules": {
    "go:S1192": {
      "level": "off"
    },
    "java:S107": {
      "level": "off"
    },
    "java:S119": {
      "level": "off"
    },
    "java:S1075": {
      "level": "off"
    },
    "java:S110": {
      "parameters": {
        "max": 8
      }
    },
    "java:S125": {
      "level": "off"
    },
    "java:S4144": {
      "level": "off"
    },
    "java:S2699": {
      "parameters": {
        "customAssertionMethods": "andExpectAll"
      }
    },
    "java:S5976": {
      "level": "off"
    },
    "java:S6813": {
      "level": "off"
    },
    "javascript:S3317": {
      "level": "off"
    },
    "typescript:S5860": {
      "level": "off"
    },
    "typescript:S125": {
      "level": "off"
    },
    "typescript:S107": {
      "level": "off"
    },
    "typescript:S3776": {
      "level": "off"
    },
    "Web:BoldAndItalicTagsCheck": {
      "level": "off"
    },
    "Web:S5256": {
      "level": "off"
    },
    "typescript:S1854": {
      "level": "off"
    },
    "typescript:S2301": {
      "level": "off"
    },
    "typescript:S4275": {
      "level": "off"
    },
    "typescript:S4323": {
      "level": "off"
    },
    "typescript:S4782": {
      "level": "off"
    },
    "typescript:S6564": {
      "level": "off"
    },
    "typescript:S6647": {
      "level": "off"
    },
    "typescript:S6479": {
      "level": "off"
    },
    "typescript:S1874": {
      "level": "off"
    },
    "Web:MouseEventWithoutKeyboardEquivalentCheck": {
      "level": "off"
    },
    "Web:S6842": {
      "level": "off"
    }
  },
  "statusbarerror.wholeLine.show": true,
  "svelte.enable-ts-plugin": true,
  "tableformatter.common.centerAlignedHeader": false,
  "tailwindCSS.includeLanguages": {
    "phoenix-heex": "html"
  },
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.confirmOnExit": "hasChildProcesses",
  "terminal.integrated.confirmOnKill": "always",
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.gpuAcceleration": "on",
  "terminal.integrated.inheritEnv": false,
  "terminal.integrated.persistentSessionScrollback": 10000,
  "terminal.integrated.scrollback": 10000,
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.highlights.enabled": true,
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.preferences.quoteStyle": "single",
  "typescript.suggest.completeFunctionCalls": true,
  "vim.camelCaseMotion.enable": true,
  "vim.leader": ",",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "vue.server.hybridMode": true,
  "window.openFoldersInNewWindow": "on",
  "workbench.colorCustomizations": {
    "editorRuler.foreground": "#333333",
    "statusBar.background": "#486faf",
    "statusBar.debuggingBackground": "#00bb73",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiBlue": "#2472c8",
    "terminal.ansiBrightBlack": "#666666",
    "terminal.ansiBrightBlue": "#3b8eea",
    "terminal.ansiBrightCyan": "#29b8db",
    "terminal.ansiBrightGreen": "#23d18b",
    "terminal.ansiBrightMagenta": "#d670d6",
    "terminal.ansiBrightRed": "#f14c4c",
    "terminal.ansiBrightWhite": "#e5e5e5",
    "terminal.ansiBrightYellow": "#f5f543",
    "terminal.ansiCyan": "#11a8cd",
    "terminal.ansiGreen": "#0dbc79",
    "terminal.ansiMagenta": "#bc3fbc",
    "terminal.ansiRed": "#cd3131",
    "terminal.ansiWhite": "#e5e5e5",
    "terminal.ansiYellow": "#e5e510",
    "terminal.background": "#1e1e1e",
    "terminal.border": "#80808059",
    "terminal.foreground": "#cccccc",
    "terminal.selectionBackground": "#ffffff40"
  },
  "workbench.colorTheme": "dark-plus-syntax",
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.showTabs": "multiple",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.startupEditor": "newUntitledFile",
  "workbench.tree.indent": 12
}
