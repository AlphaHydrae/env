{
  "better-comments.tags": [
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      "tag": "fixme",
      "color": "#c00000",
      "strikethrough": false,
      "backgroundColor": "transparent"
    }
  ],
  "debug.console.fontSize": 16,
  "editor.detectIndentation": false,
  "editor.fontSize": 16,
  "editor.fontFamily": "BitStream Vera Sans Mono, Menlo, Monaco, 'Courier New', monospace",
  "editor.renderWhitespace": "boundary",
  "editor.scrollBeyondLastLine": false,
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "[dark-plus-syntax]": {
      "comments": "#6A9955"
    }
  },
  "files.associations": {
    "Vagrantfile": "ruby"
  },
  "gitlens.currentLine.enabled": false,
  "javascript.preferences.quoteStyle": "single",
  "statusbarerror.wholeLine.show": true,
  "tableformatter.common.centerAlignedHeader": false,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.inheritEnv": false,
  "todo-tree.highlights.enabled": true,
  "typescript.preferences.quoteStyle": "single",
  "vim.camelCaseMotion.enable": true,
  "vim.leader": ",",
  "window.openFoldersInNewWindow": "on",
  "window.zoomLevel": 0,
  "workbench.colorCustomizations": {
    "statusBar.background": "#486faf"
  },
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.showTabs": true,
  "workbench.tree.indent": 12,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.colorTheme": "dark-plus-syntax"
}
