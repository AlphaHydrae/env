{
  "[elixir]": {
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.formatOnType": false,
    "editor.trimAutoWhitespace": true,
    "editor.wordBasedSuggestions": true,
    "files.insertFinalNewline": true
  },
  "better-comments.tags": [
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      "tag": "fixme",
      "color": "#c00000",
      "strikethrough": false,
      "backgroundColor": "transparent"
    }
  ],
  "debug.console.fontSize": 16,
  "editor.detectIndentation": false,
  "editor.fontSize": 16,
  "editor.fontFamily": "BitStream Vera Sans Mono, Menlo, Monaco, 'Courier New', monospace",
  "editor.renderWhitespace": "boundary",
  "editor.scrollBeyondLastLine": false,
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "[dark-plus-syntax]": {
      "comments": "#6A9955"
    }
  },
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "Vagrantfile": "ruby"
  },
  "gitlens.currentLine.enabled": false,
  "javascript.preferences.quoteStyle": "single",
  "statusbarerror.wholeLine.show": true,
  "tableformatter.common.centerAlignedHeader": false,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.inheritEnv": false,
  "todo-tree.highlights.enabled": true,
  "typescript.preferences.quoteStyle": "single",
  "vim.camelCaseMotion.enable": true,
  "vim.leader": ",",
  "window.openFoldersInNewWindow": "on",
  "window.zoomLevel": 0,
  "workbench.colorCustomizations": {
    "statusBar.background": "#486faf",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiBlue": "#2472c8",
    "terminal.ansiBrightBlack": "#666666",
    "terminal.ansiBrightBlue": "#3b8eea",
    "terminal.ansiBrightCyan": "#29b8db",
    "terminal.ansiBrightGreen": "#23d18b",
    "terminal.ansiBrightMagenta": "#d670d6",
    "terminal.ansiBrightRed": "#f14c4c",
    "terminal.ansiBrightWhite": "#e5e5e5",
    "terminal.ansiBrightYellow": "#f5f543",
    "terminal.ansiCyan": "#11a8cd",
    "terminal.ansiGreen": "#0dbc79",
    "terminal.ansiMagenta": "#bc3fbc",
    "terminal.ansiRed": "#cd3131",
    "terminal.ansiWhite": "#e5e5e5",
    "terminal.ansiYellow": "#e5e510",
    "terminal.background": "#1e1e1e",
    "terminal.border": "#80808059",
    "terminal.foreground": "#cccccc",
    "terminal.selectionBackground": "#ffffff40"
  },
  "workbench.colorTheme": "dark-plus-syntax",
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.showTabs": true,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.tree.indent": 12
}
